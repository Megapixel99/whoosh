{
  "name": "whoosh",
  "version": "1.4.0",
  "description": "A streaming sftp client",
  "main": "index.js",
  "scripts": {
    "test": "zunit",
    "lint": "eslint .",
    "coverage": "nyc --report html --reporter lcov --reporter text-summary zunit",
    "codeclimate": "(test -e .codeclimate && npm run coverage && source .codeclimate) || echo skipping codeclimate",
    "precommit": "npm run lint && npm test",
    "prepush": "npm run codeclimate",
    "docker": "docker-compose up -d"
  },
  "keywords": [
    "sftp",
    "streaming",
    "client"
  ],
  "engines": {
    "node": ">=10"
  },
  "author": "GuideSmiths",
  "license": "ISC",
  "devDependencies": {
    "async": "^3.2.4",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-imperative": "^2.1.0",
    "eslint-plugin-imperative": "^2.1.0",
    "fs-extra": "^7.0.1",
    "husky": "^1.3.1",
    "nyc": "^15.1.0",
    "zunit": "^3.2.1"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "lodash": "^4.17.21",
    "ssh2": "^1.10.0"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guidesmiths/whoosh.git"
  },
  "bugs": {
    "url": "https://github.com/guidesmiths/whoosh/issues"
  },
  "zUnit": {
    "pollute": true
  },
  "homepage": "https://guidesmiths.github.io/whoosh/"
}
